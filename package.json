    "deploy": "yarn bump-version && yarn build && push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "favicons": "^7.2.0",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.16.11",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/tsconfig": "^0.5.1",
    "npm-run-all2": "^6.2.0",
    "push-dir": "^0.4.1",
    "sass-embedded": "^1.79.3",
    "tsx": "^4.19.1",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vue-tsc": "^2.0.21"
